                .main-entries__text
                  .main-entries__title
                    %h3 alii
              %a.main-entries__item{ 'data-target': ".pokebolws-modals", 'data-toggle': 'modal', href: "" }
                .main-entries__img
                  %img{ :src => "assets/img/Poke_bowls/fondo azul agua marina.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 luau
              %a.main-entries__item{ 'data-target': ".pokebolws-modals", 'data-toggle': 'modal', href: "" }
                .main-entries__img
                  %img{ :src => "assets/img/Poke_bowls/fondo azul.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 mahalo
              %a.main-entries__item{ 'data-target': ".pokebolws-modals", 'data-toggle': 'modal', href: "" }
                .main-entries__img
                  %img{ :src => "assets/img/Poke_bowls/fondo amarillo pollo.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 palenq
          #v-pills-contact.tab-pane.fade
            .main-pokebowls__content
              %a.main-entries__item{ 'data-target': ".juicebar-modals", 'data-toggle': 'modal', href: ""}
                .main-entries__img
                  %img{ :src => "assets/img/Juice_bar/limonada.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 limonada
              %a.main-entries__item{ 'data-target': ".juicebar-modals", 'data-toggle': 'modal', href: ""}
                .main-entries__img
                  %img{ :src => "assets/img/Juice_bar/jugo mandarina.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 mandarina
              %a.main-entries__item{ 'data-target': ".juicebar-modals", 'data-toggle': 'modal', href: ""}
                .main-entries__img
                  %img{ :src => "assets/img/Juice_bar/Carrott Blast.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 carrot blast
              %a.main-entries__item{ 'data-target': ".juicebar-modals", 'data-toggle': 'modal', href: ""}

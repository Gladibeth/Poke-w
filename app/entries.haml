              %a.main-entries__item{ 'data-target': ".entries-modals", 'data-toggle': 'modal', href: "" }
                .main-entries__img
                  %img{ :src => "assets/img/Menu_entradas/tartar.png"}/
                .main-entries__text
                  .main-entries__title
                    %h3 tartar
          #v-pills-profile.tab-pane.fade.show.active
            .main-pokebowls__content
              %a.main-entries__item{ 'data-target': ".pokebolws-modals", 'data-toggle': 'modal', href: "" }
                .main-entries__img
                  %img{ :src => "assets/img/Poke_bowls/fondo amarillo.png"}/

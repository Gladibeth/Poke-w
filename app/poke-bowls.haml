!!!
%html{lang: "en"}
  %head
    %meta{charset: "UTF-8"}/
    %meta{content: "width=device-width, initial-scale=1.0", name:"viewport"}/
    %title Poke
    %link{rel: "stylesheet", href:"assets/css/bootstrap.min.css"}/
    %link{rel: "stylesheet", href:"assets/css/main.css"}/
    %link{rel: "stylesheet", href:"assets/css/slick.css"}/
    %link{rel: "stylesheet", href:"assets/css/slick-theme.css"}/
    %link{rel: "stylesheet", href:"assets/css/datepicker.css"}/
  %body
    %header#home
      %nav.navbar.navbar-expand-lg.fixed-top.navbar-fixed-js
        .container
          .main-brand
            %a.navbar-brand{:href => "index.html"}
              %img#iso{:src => "assets/img/Home/Logo poke.svg"}/
            %button.navbar-toggler.p-2.border-0.hamburger.hamburger--elastic.ml-autos{"data-toggle" => "offcanvas", :type => "button"}
              %span.hamburger-box
                %span.hamburger-inner
          .navbar-collapse.offcanvas-collapse
            %ul.navbar-nav.mr-autos
              %li.nav-item.active
                %a.nav-link.active{:href => "entries.html"} Menu
              %li.nav-item
                %a.nav-link{:href => "about.html"} Nosotros
              %li.nav-item
                %a.nav-link{:href => "#"} Ubicaciones
              %li.nav-item
                %a.nav-link{:href => "poke-dots.html"} Poke puntos
              %li.nav-item
                %a.nav-link{:href => "environmental.html"} Ambiental
              %li.nav-item
                %a.nav-link{:href => "#"} Eventos
              %li.nav-item
                .main-nav__btn
                  .btn__buy
                    %a.btn-btn_custom.btn--medium.btn--filled.btn--filled--buy{:href => "https://www.rappi.com.co/"} 
                      %img{ :src => "assets/img/Home/R Rappi.svg"}/
                      %span.line
                      Pide en línea
    .about-banner{style:"background-image: url('assets/img/Poke_bowls/POKE_-249.png');"}
      .overlay
      .about-banner__text.about-banner__text--center
        .about-banner__title
          %h3 menu
    .main-entries
      .container
        .main-entries__nav
          %ul#pills-tab.nav.nav-pills.mb-3{:role => "tablist"}
            %li.nav-item.nav-item--tabs
              %a#pills-home-tab.nav-link--tabs{ :href => "entries.html"} entradas
            %li.nav-item.nav-item--tabs
              %a#pills-profile-tab.nav-link--tabs.active{:href => "poke-bowls.html"} poke bowls
            %li.nav-item.nav-item--tabs
              %a#pills-contact-tab.nav-link--tabs{:href => "juice-bar.html"} juice bar
          .btn__tabs
            %a.btn-btn_custom.btn--large.btn--filled.btn--filled-gray{:href => ""} 
              %img{ :src => "assets/img/Home/R Rappi.svg"}/
              %span.line
              ARMA TU BOWL
        %hr.line-gray.line-gray--entries
        .main-pokebowls__content
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo amarillo.png"}/
            .main-entries__text
              .main-entries__title
                %h3 shaka
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo naranja 1.png"}/
            .main-entries__text
              .main-entries__title
                %h3 pulled pork
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo piel.png"}/
            .main-entries__text
              .main-entries__title
                %h3 alii
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo azul agua marina.png"}/
            .main-entries__text
              .main-entries__title
                %h3 luau
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo azul.png"}/
            .main-entries__text
              .main-entries__title
                %h3 mahalo
          .main-entries__item
            .main-entries__img
              %img{ :src => "assets/img/Poke_bowls/fondo amarillo pollo.png"}/
            .main-entries__text
              .main-entries__title
                %h3 palenq

        
        
        
    %footer.main-footer
      .container
        .main-footer__content
          .main-footer__item
            .main-footer__box
              .main-footer__logo
                %img{ :src => "assets/img/Home/Logo poke.svg"}/
              .main-footer__boxsmall
                .main-footer__boxitem
                  %img{ :src => "assets/img/Home/Endeavor.svg"}/
                .main-footer__boxitem
                  .main-footer__text
                    %p Part of Foodie Group S.A.S & Endeavor company
              .main-footer__address
                %p Cl. 69a #No.6-37 - Bogotá, Colombia
          .main-footer__item
            .main-footer__box
              .main-footer__title
                %p mas informacion
              %ul
                %li 
                  %a{ :href=>"#"}
                    ESCRÍBENOS
                %li 
                  %a{ :href=>"#"}
                    TRABAJA CON NOSOTROS
                %li 
                  %a{ :href=>"#"}
                    CUÉNTANOS TU EXPERIENCIA
                %li 
                  %a{ :href=>"#"}
                    TERMINOS Y CONDICIONES
                %li 
                  %a{ :href=>"#"}
                    POLITICA DE PRIVACIDAD
              
          .main-footer__item
            .main-footer__box
              .main-footer__title
                %p NEWSLETTER
              .main-footer__subscribe
                %form
                  .form-group
                    %input#formGroupExampleInput.form-control{:placeholder => "Correo electronico", :type => "email"}/
                  .main-footer__btn
                    %button.btn_custom.btn--medium.btn--filled-gray
                      Enviar
          .main-footer__item
            .main-footer__box
              .main-footer__redes
                %a{ :href=>""}
                  %img{ :src => "assets/img/Home/f_logo_RGB-White_1024_1024.svg"}/
                %a{ :href=>""}
                  %img{ :src => "assets/img/Home/glyph-logo_IG-01.svg"}/
                %a{ :href=>""}
                  %img{ :src => "assets/img/Home/linked in logo circulo.svg"}/
                %a{ :href=>""}
                  %img{ :src => "assets/img/Home/Twitter_Logo_WhiteOnImage.svg"}/
              .main-footer__email
                %p servicios@pokecolombia.co
      .main-footer__copy
        %p Poke colombia, 2019 - Todos los derechos Reservados
                  
                  

    %script{src: "assets/js/jquery.min.js"}
    %script{src: "assets/js/bootstrap.min.js"}
    %script{src: "assets/js/slick.min.js"}
    %script{src: "assets/js/setting-slick.js"}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.js"}
    %script{src: "assets/js/main.js"}

